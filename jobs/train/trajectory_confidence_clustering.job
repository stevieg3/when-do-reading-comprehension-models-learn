#!/bin/bash -l

#$ -cwd
#$ -S /bin/bash
#$ -l tmem=16G
#$ -t 1-27
#$ -l h_rt=48:00:00
#$ -o /SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn/array_files
#$ -e /SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn/array_files

hostname
date

# Activate conda environment
conda activate rclearn

export LANG="en_US.utf8"
export LANGUAGE="en_US:en"
export PYTHONPATH=/SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn
export HF_HOME=/SAN/intelsys/rclearn/cache

cd /SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn

test $SGE_TASK_ID -eq 1 && sleep 10 && python src/models/kmeans_conf.py --seed 27 --km_seed 27 --n_clusters 4 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=27-km_seed=27-n_clusters=4.csv
test $SGE_TASK_ID -eq 2 && sleep 10 && python src/models/kmeans_conf.py --seed 27 --km_seed 27 --n_clusters 7 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=27-km_seed=27-n_clusters=7.csv
test $SGE_TASK_ID -eq 3 && sleep 10 && python src/models/kmeans_conf.py --seed 27 --km_seed 27 --n_clusters 10 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=27-km_seed=27-n_clusters=10.csv
test $SGE_TASK_ID -eq 10 && sleep 10 && python src/models/kmeans_conf.py --seed 28 --km_seed 27 --n_clusters 4 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=28-km_seed=27-n_clusters=4.csv
test $SGE_TASK_ID -eq 11 && sleep 10 && python src/models/kmeans_conf.py --seed 28 --km_seed 27 --n_clusters 7 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=28-km_seed=27-n_clusters=7.csv
test $SGE_TASK_ID -eq 12 && sleep 10 && python src/models/kmeans_conf.py --seed 28 --km_seed 27 --n_clusters 10 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=28-km_seed=27-n_clusters=10.csv
test $SGE_TASK_ID -eq 19 && sleep 10 && python src/models/kmeans_conf.py --seed 29 --km_seed 27 --n_clusters 4 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=29-km_seed=27-n_clusters=4.csv
test $SGE_TASK_ID -eq 20 && sleep 10 && python src/models/kmeans_conf.py --seed 29 --km_seed 27 --n_clusters 7 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=29-km_seed=27-n_clusters=7.csv
test $SGE_TASK_ID -eq 21 && sleep 10 && python src/models/kmeans_conf.py --seed 29 --km_seed 27 --n_clusters 10 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-confidence-clusters-seed=29-km_seed=27-n_clusters=10.csv

date
