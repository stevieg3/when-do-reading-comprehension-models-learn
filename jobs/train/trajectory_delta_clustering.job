test $SGE_TASK_ID -eq 3 && sleep 10 && python src/models/kmeans_delta.py --seed 27 --km_seed 27 --n_clusters 10 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-delta-clusters-seed=27-km_seed=27-n_clusters=10.csv'
