#!/bin/bash -l

#$ -cwd
#$ -S /bin/bash
#$ -l tmem=16G
#$ -t 1-3
#$ -l h_rt=48:00:00
#$ -o /SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn/array_files
#$ -e /SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn/array_files

hostname
date

# Activate conda environment
conda activate rclearn

export LANG="en_US.utf8"
export LANGUAGE="en_US:en"
export PYTHONPATH=/SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn
export HF_HOME=/SAN/intelsys/rclearn/cache

cd /SAN/intelsys/rclearn/when-do-reading-comprehension-models-learn

test $SGE_TASK_ID -eq 1 && sleep 10 && python src/models/kmeans_delta.py --seed 27 --km_seed 27 --n_clusters 4 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-delta-clusters-seed=27-km_seed=27-n_clusters=4.csv'
