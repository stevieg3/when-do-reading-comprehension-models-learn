# test $SGE_TASK_ID -eq 7 && sleep 10 && python src/models/kmeans_delta.py --seed 29 --km_seed 27 --n_clusters 4 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-delta-clusters-seed=29-km_seed=27-n_clusters=4.csv'
# test $SGE_TASK_ID -eq 8 && sleep 10 && python src/models/kmeans_delta.py --seed 29 --km_seed 27 --n_clusters 7 --max_iter 300 --savepath 'data/processed/squadv1-adversarialall-delta-clusters-seed=29-km_seed=27-n_clusters=7.csv'
